@{
    ViewBag.Title = "Izpis";
}

<h2>Izpis uvoženih študentov</h2>

<table id="izpis">
    <thead>
        <tr>
            <th>Ime</th>
            <th>Priimek</th>
            <th>Program</th>
            <th>Email</th>
            <th>Uporabniško ime</th>
            <th>Geslo</th>
            <th>Dodan</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < @ViewBag.CounterAll; i++)
        {
            <tr>
                <td>@ViewBag.Name[i]</td>
                <td>@ViewBag.Lname[i]</td>
                <td>@ViewBag.Program[i]</td>
                <td>@ViewBag.Email[i]</td>
                <td>@ViewBag.Username[i]</td>
                <td>@ViewBag.Password[i]</td>
                <td>@ViewBag.Added[i]</td>
            </tr>
        }
    </tbody>
    <tfoot>
        <tr>
            <td colspan="7">Dodanih @ViewBag.Counter od @ViewBag.CounterAll</td>
        </tr>
    </tfoot>
</table>
