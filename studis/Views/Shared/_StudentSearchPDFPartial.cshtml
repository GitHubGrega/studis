@model IEnumerable<studis.Models.vpisnilist>


@using (Html.BeginForm())
{
    <p>
        @Html.TextBox("SearchString1")
        <input type="submit" value="Išči" />
    </p>
}
<div id="searchDiv">
    <table id="seznamStudentov" class="table">
        <tr>
            <th>Štud.leto</th>
            <th>Vpisna Številka</th>
            <th>Ime</th>
            <th>Priimek</th>
            <th>Izvoz</th>
        </tr>
        @if (Model != null)
        {
            foreach (var item in Model)
            {
                var imeID = item.ime;
                <tr>
                    <td>@Html.DisplayFor(modelItem => item.studijskoLeto)</td>
                    <td>@Html.DisplayFor(modelItem => item.vpisnaStevilka)</td>
                    <td>@Html.DisplayFor(modelItem => item.ime)</td>
                    <td>@Html.DisplayFor(modelItem => item.priimek)</td>
                    <td>@Html.ActionLink("Izvoz", "Pdf", "List", new { id = item.id }, null)</td>
                </tr>
            }
        }
    </table>
@if (Model == null)
{
    <h4>Ni zadetkov!</h4>
    <p>Uporaba: iskanje s celo vpisno številko ali začetnicami imena in priimka.</p>
}

</div>
