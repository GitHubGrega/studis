@model studis.Models.predmet

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Izberite predmet</h2>

@using (Html.BeginForm()) 
{
    <fieldset>
        <div class="editor-label">
            Izberite predmet
        </div>
        <div class="editor-field">
            @Html.DropDownListFor(m => m.id, (SelectList)ViewBag.Seznam)
        </div>
        <div class="editor-label">
            Izberite študijsko leto
        </div>
        <div class="editor-field">
            @Html.DropDownList("Value",ViewData["sList"] as SelectList)
        </div>
        <p>
            <input type="submit" value="Seznam študentov">
        </p>
    </fieldset>
}
